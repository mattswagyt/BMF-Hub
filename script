-- BMF Hub

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "BMF Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local GamesTab = Window:MakeTab({
    Name = "Games",
    Icon = "",
    PremiumOnly = false
})

