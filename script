-- Local Player Tab
local LocalPlayerSection = LocalPlayerTab:AddSection("Player Modifications", 1)

LocalPlayerSection:AddSlider({
    enabled = true,
    text = "WalkSpeed",
    tooltip = "Change your walkspeed",
    flag = "WalkSpeedSlider",
    min = 16,
    max = 300,
    increment = 1,
    callback = function(v)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
    end
})

LocalPlayerSection:AddSlider({
    enabled = true,
    text = "JumpPower",
    tooltip = "Change your jump power",
    flag = "JumpPowerSlider",
    min = 50,
    max = 500,
    increment = 10,
    callback = function(v)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
    end
})

-- Server Tab
local ServerSection = ServerTab:AddSection("Server Actions", 1)

-- Rejoin Logic
ServerSection:AddButton({
    enabled = true,
    text = "Rejoin",
    tooltip = "Rejoin the current server",
    confirm = true,
    callback = function()
        local player = game.Players.LocalPlayer
        if player then
            game:GetService("TeleportService"):Teleport(game.PlaceId, player)
        else
            library:SendNotification("Failed to rejoin: Player not found!", 3)
        end
    end
})

-- Server Hop Logic
ServerSection:AddButton({
    enabled = true,
    text = "Server Hop",
    tooltip = "Join a different server",
    confirm = true,
    callback = function()
        local HttpService = game:GetService("HttpService")
        local TeleportService = game:GetService("TeleportService")
        local PlaceId = game.PlaceId

        -- Fetch a list of servers from Roblox's API
        local servers = {}
        local cursor = ""
        local success, errorMessage = pcall(function()
            repeat
                local response = HttpService:JSONDecode(game:HttpGet(
                    "https://games.roblox.com/v1/games/" .. PlaceId .. "/servers/Public?sortOrder=Asc&limit=100&cursor=" .. cursor
                ))
                for _, server in pairs(response.data) do
                    if server.playing < server.maxPlayers and server.id ~= game.JobId then
                        table.insert(servers, server.id)
                    end
                end
                cursor = response.nextPageCursor
            until not cursor
        end)

        if not success then
            library:SendNotification("Failed to fetch servers: " .. errorMessage, 3)
            return
        end

        -- Attempt to hop to a new server
        if #servers > 0 then
            local randomServerId = servers[math.random(1, #servers)]
            TeleportService:TeleportToPlaceInstance(PlaceId, randomServerId, game.Players.LocalPlayer)
        else
            library:SendNotification("No servers available for hopping!", 3)
        end
    end
})

-- Misc Tab
