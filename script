local PlayerTab = Window:MakeTab({
    Name = "Local Player",
    Icon = "",
    PremiumOnly = false
})

local PlayerSection = PlayerTab:AddSection({
    Name = "Movement"
})

-- Walkspeed Slider
PlayerTab:AddSlider({
    Name = "Walkspeed",
    Min = 16,
    Max = 500,
    Default = 16,
    Color = Color3.fromRGB(255, 255, 255),
    Increment = 1,
    ValueName = "WS",
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end
})

-- Jump Height Slider
PlayerTab:AddSlider({
    Name = "Jump Height",
    Min = 16,
    Max = 500,
    Default = 50, -- Updated default for Jump Height to 50
    Color = Color3.fromRGB(255, 255, 255),
    Increment = 1,
    ValueName = "Height",
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
    end
})

-- Reset Character Button
PlayerTab:AddButton({
    Name = "Reset Character",
    Callback = function()
        game.Players.LocalPlayer.Character:BreakJoints() -- Breaks character joints to reset
    end
})

-- Variable to store Noclip connection
local noclipConnection

-- Noclip Button
PlayerTab:AddButton({
    Name = "Noclip",
    Callback = function()
        -- Start noclip by disabling collisions
        noclipConnection = game:GetService("RunService").Stepped:Connect(function()
            for _, part in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end)
    end
})

-- Unclip Button
PlayerTab:AddButton({
    Name = "Unclip",
    Callback = function()
        -- Disconnect the Noclip loop if it exists
        if noclipConnection then
            noclipConnection:Disconnect()
            noclipConnection = nil
        end
        
        -- Reset all parts to allow collisions again
        for _, part in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = true
            end
        end
    end
})

-- Fly Variables
local FLYING = false
local BG, BV
local speed = 50 -- Flight speed
local T = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")

-- Fly Button
PlayerTab:AddButton({
    Name = "Fly",
    Callback = function()
        if not FLYING then
            FLYING = true
            BG = Instance.new('BodyGyro')
            BV = Instance.new('BodyVelocity')
            
            BG.P = 9e4
            BG.Parent = T
            BV.Parent = T
            BG.maxTorque = Vector3.new(9e9, 9e9, 9e9)
            BG.cframe = T.CFrame
            BV.velocity = Vector3.new(0, 0, 0)
            BV.maxForce = Vector3.new(9e9, 9e9, 9e9)

            task.spawn(function()
                while FLYING do
                    wait()
                    
                    -- Controls for flying
                    local moveDirection = Vector3.new(0, 0, 0)
                    local player = game.Players.LocalPlayer
                    local humanoid = player.Character:FindFirstChild("Humanoid")

                    -- Use WASD keys for movement
                    if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.W) then
                        moveDirection = moveDirection + T.CFrame.LookVector
                    end
                    if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.A) then
                        moveDirection = moveDirection - T.CFrame.RightVector
                    end
                    if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.S) then
                        moveDirection = moveDirection - T.CFrame.LookVector
                    end
                    if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.D) then
                        moveDirection = moveDirection + T.CFrame.RightVector
                    end

                    -- Use Q and E keys for altitude control
                    if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.Q) then
                        moveDirection = moveDirection + Vector3.new(0, -1, 0)
                    end
                    if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.E) then
                        moveDirection = moveDirection + Vector3.new(0, 1, 0)
                    end
                    
                    -- Apply movement
                    BV.velocity = moveDirection.Unit * speed -- Scale movement based on speed
                    BG.cframe = T.CFrame -- Keep the BodyGyro aligned to the character's orientation
                end
            end)
        end
    end
})

-- Unfly Button
PlayerTab:AddButton({
    Name = "Unfly",
    Callback = function()
        -- Stop Flying Logic
        if FLYING then
            FLYING = false
            BG:Destroy()
            BV:Destroy()

            -- Reset the player's position and stop applying any forces
            T.Velocity = Vector3.new(0, 0, 0) -- Stop any upward/downward motion
        end
    end
})

-- God Mode Button
PlayerTab:AddButton({
    Name = "God Mode (May not work on all games)",
    Callback = function()
        -- Get the character and humanoid
        local character = game.Players.LocalPlayer.Character
        local humanoid = character:FindFirstChildOfClass("Humanoid")

        if humanoid then
            -- Disable Health changes and set it to a very high value
            humanoid.Health = math.huge
            humanoid.MaxHealth = math.huge

            -- Prevent death from any damage source (including "kill all")
            humanoid.Died:Connect(function()
                humanoid.Health = math.huge -- Reset health to infinite on death attempt
            end)

            -- Block damage events completely
            local oldHealth = humanoid.Health
            local healthChangedConn
            healthChangedConn = humanoid.HealthChanged:Connect(function()
                if humanoid.Health < oldHealth then
                    humanoid.Health = math.huge -- Reset to infinite if health decreases
                end
            end)

            -- Prevent the health bar from showing 0, and disable health bar changes
            local healthBar = humanoid:FindFirstChild("HealthDisplay")
            if healthBar then
                healthBar.Visible = false -- Hide the health bar (optional)
            end
        end

        -- Disable other forms of damage interactions, like team kills
        local function preventTeamKills()
            local teams = game:GetService("Teams")
            local playerTeam = game.Players.LocalPlayer.Team

            -- Ensure no one on the opposite team can damage you
            local function onPlayerDamage(damage)
                if game.Players.LocalPlayer.Team ~= playerTeam then
                    return 0 -- No damage if they are on the opposite team
                end
            end

            -- Listen for all incoming damage attempts (e.g., from guards or enemies)
            game.Players.LocalPlayer.Character.Humanoid.HealthChanged:Connect(function()
                -- Recheck the player's team and prevent damage if needed
                if game.Players.LocalPlayer.Team ~= playerTeam then
                    -- Prevent incoming damage from opposite teams
                    return 0
                end
            end)
        end

        -- Run the prevent team kills function
        preventTeamKills()
    end
})



-- Other tab

local OtherTab = Window:MakeTab({
    Name = "Misc",
    Icon = "",
    PremiumOnly = false
})

local OtherSection = OtherTab:AddSection({
    Name = "Other"
})

OtherTab:AddButton({
    Name = "Delete Doors",
    Callback = function()
        game.Workspace.Doors:Destroy()
    end    
})

OtherTab:AddButton({
    Name = "Delete Walls",
    Callback = function()
        local walls = game.Workspace:FindPartsInRegion3(Region3.new(Vector3.new(-1000, -1000, -1000), Vector3.new(1000, 1000, 1000)), nil, math.huge)
        for _, part in ipairs(walls) do
            if part.Name == "Wall" then
                part:Destroy()
            end
        end
    end    
})

OtherTab:AddButton({
    Name = "ESP",
    Callback = function()
        for _, player in ipairs(game.Players:GetPlayers()) do
            if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                local espBox = Instance.new("BoxHandleAdornment")
                espBox.Size = player.Character.HumanoidRootPart.Size + Vector3.new(0.1, 0.1, 0.1)
                espBox.Adornee = player.Character.HumanoidRootPart
                espBox.AlwaysOnTop = true
                espBox.Color3 = Color3.fromRGB(255, 0, 0) -- Red color
                espBox.Transparency = 0.5
                espBox.ZIndex = 5
                espBox.Parent = game.Workspace.CurrentCamera
            end
        end
    end
})

OtherTab:AddButton({
    Name = "Rejoin Game",
    Callback = function()
        game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, game.JobId)
    end    
})


