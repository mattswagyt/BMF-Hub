    Name = "Server Hop",
    Callback = function()
        local servers = game:GetService("TeleportService"):GetPlayerPlaceInstanceAsync(game.Players.LocalPlayer.UserId)
        game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, servers[math.random(#servers)].Id)
    end
})

